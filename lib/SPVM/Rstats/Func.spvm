use Math;

package Rstats::Func {
  
  sub sin_double($ndarray_in : Rstats::NDArray::Double, $ndarray_out : Rstats::NDArray::Double) : void {
    
    my $values_in = $ndarray_in->{values};
    my $values_out = $ndarray_out->{values};
    
    if (len $values_in != len $values_out) {
      die "Elements count must be same";
    }
    
    for (my $i = 0; $i < @$values_in; $i++) {
      $values_out->[$i] = Math::sin($values_in->[$i]);
    }
  }
}
